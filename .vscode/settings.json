{
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
    ],
    "[javascript]": {
        "editor.formatOnSave": false
    },
    "[javascriptreact]": {
        "editor.formatOnSave": false
    },
    "[typescript]": {
        "editor.formatOnSave": false
    },
    "[typescriptreact]": {
        "editor.formatOnSave": false
    },
    "[json]": {
        "files.insertFinalNewline": true,
        "files.trimTrailingWhitespace": true
    },
    "[jsonc]": {
        "files.insertFinalNewline": true,
        "files.trimTrailingWhitespace": true
    },
    "[html]": {
        "files.insertFinalNewline": true,
        "files.trimTrailingWhitespace": true
    },
    "[css]": {
        "files.insertFinalNewline": true,
        "files.trimTrailingWhitespace": true
    },
    "[ignore]": {
        "files.insertFinalNewline": true,
        "files.trimTrailingWhitespace": true
    }
}
